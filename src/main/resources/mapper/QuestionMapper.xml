<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiaou.tktiku.mapper.QuestionMapper">

    <resultMap id="BaseResultMap" type="com.xiaou.tktiku.entity.Question">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="bankId" column="bank_id" jdbcType="BIGINT"/>
            <result property="title" column="title" jdbcType="VARCHAR"/>
            <result property="contentMd" column="content_md" jdbcType="VARCHAR"/>
            <result property="difficulty" column="difficulty" jdbcType="TINYINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,bank_id,title,
        content_md,difficulty
    </sql>
</mapper>
